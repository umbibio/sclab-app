{
    "menu_name": "SCLab-App",
    "menu_items": [
        {
            "name": "SCLab-App",
            "description": "Interactive single-cell analysis toolkit with JupyterLab",
            "command": [
                "{{ PYTHON }}",
                "-m",
                "sclab_app.cli",
                "main",
                "--open-delay",
                "3"
            ],
            "working_dir": "{{ HOME }}/Documents/SCLab-App",
            "activate": true,
            "terminal": true,
            "platforms": {
                "win": {
                    "icon": "{{ PREFIX }}/share/sclab-app/sclab-app.ico",
                    "desktop": false,
                    "app_user_model_id": "UMBibio.SCLabApp.Main"
                },
                "linux": {
                    "icon": "{{ PREFIX }}/share/sclab-app/sclab-app_256x256.png",
                    "Categories": [
                        "Science",
                        "Education",
                        "Development"
                    ],
                    "Keywords": [
                        "single-cell",
                        "bioinformatics",
                        "jupyter",
                        "analysis"
                    ],
                    "GenericName": "Single-Cell Analysis Lab",
                    "StartupNotify": true
                },
                "osx": {
                    "icon": "{{ PREFIX }}/share/sclab-app/sclab-app.icns",
                    "CFBundleIdentifier": "org.umbibio.sclab-app.main",
                    "CFBundleDisplayName": "SCLab-App",
                    "LSApplicationCategoryType": "public.app-category.education"
                }
            }
        },
